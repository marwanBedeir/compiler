               <children>
                  <ScrollPane fitToHeight="true" fitToWidth="true" focusTraversable="false" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="1200.0">
                     <content>
                        <HBox minWidth="1000.0" prefHeight="100.0" prefWidth="200.0">
